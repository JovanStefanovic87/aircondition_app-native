{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-native",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES6"],
    "typeRoots": ["./node_modules/@types"],
    "allowJs": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/components/CustomIcon.ts"],
  "exclude": ["node_modules"]
}
